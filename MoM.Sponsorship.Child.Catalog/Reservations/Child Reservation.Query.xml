<QueryViewSpec 
    xmlns:c="bb_appfx_commontypes"
    ID="3fd5160e-ddc9-4479-a54f-ba554bc099e5"
    Name="Child Reservation"
    Description="Provides support for querying on Child Reservation information."
    Author="BETHESDA\cmayeda"
    PrimaryKeyField="ID"
    RecordType="Child Reservation"
    IsRoot="true"
    c:SecurityUIFolder="Child Reservation"
    xmlns="bb_appfx_queryview"
    >
    <ViewImplementation ViewName="USR_V_QUERY_USR_CHILDRESERVATION">
        <ViewSQL>
<![CDATA[
select
    USR_CHILDRESERVATION.ID,
    USR_CHILDRESERVATION.[SPONSORSHIPOPPORTUNITYCHILDID],
    USR_CHILDRESERVATION.[SPONSORSHIPOPPORTUNITYRESERVEPROCESSID],
    USR_CHILDRESERVATION.[SOLICITORID],
    USR_CHILDRESERVATION.[STARTDATE],
    USR_CHILDRESERVATION.[ENDDATE],
    USR_CHILDRESERVATION.[STATUSCODE],
	ADDEDBY.APPLICATIONNAME as [ADDEDBY_APPLICATION],
	ADDEDBY.USERNAME as [ADDEDBY_USERNAME],
	CHANGEDBY.APPLICATIONNAME as [CHANGEDBY_APPLICATION],
	CHANGEDBY.USERNAME as [CHANGEDBY_USERNAME],
	USR_CHILDRESERVATION.DATEADDED,
	USR_CHILDRESERVATION.DATECHANGED,
	USR_CHILDRESERVATION.TSLONG
from 
    dbo.USR_CHILDRESERVATION
    left join dbo.CHANGEAGENT as [ADDEDBY] on [ADDEDBY].ID = USR_CHILDRESERVATION.ADDEDBYID
    left join dbo.CHANGEAGENT as [CHANGEDBY] on [CHANGEDBY].ID = USR_CHILDRESERVATION.CHANGEDBYID
]]>
</ViewSQL>
    </ViewImplementation>
    <Output>
        <OutputFields>
            <OutputField Name="ID" Caption="System record ID" Category="System Fields" DataType="Guid" />
            <OutputField Name="SPONSORSHIPOPPORTUNITYCHILDID" Caption="Sponsorshipopportunitychild" DataType="Guid" />
            <OutputField Name="SPONSORSHIPOPPORTUNITYRESERVEPROCESSID" Caption="Sponsorshipopportunityreserveprocess" DataType="Guid" />
            <OutputField Name="SOLICITORID" Caption="Solicitor" DataType="Guid" />
            <OutputField Name="STARTDATE" Caption="Startdate" DataType="Date" />
            <OutputField Name="ENDDATE" Caption="Enddate" DataType="Date" />
            <OutputField Name="STATUSCODE" Caption="Status" DataType="TinyInt" />
            <OutputField Name="ADDEDBY_APPLICATION" Caption="Added by application" Category="System Fields">
                <LookupInfo>
                    <SimpleDataList SimpleDataListID="37e3e458-af0d-4dbc-8a18-a93885521a42" />
                </LookupInfo>
            </OutputField>
            <OutputField Name="ADDEDBY_USERNAME" Caption="Added by user name" Category="System Fields">
                <LookupInfo>
                    <SimpleDataList SimpleDataListID="cea15e1c-e455-41be-9ecf-6b5453fa96a4" />
                </LookupInfo>
            </OutputField>
            <OutputField Name="CHANGEDBY_APPLICATION" Caption="Changed by application" Category="System Fields">
                <LookupInfo>
                    <SimpleDataList SimpleDataListID="37e3e458-af0d-4dbc-8a18-a93885521a42" />
                </LookupInfo>
            </OutputField>
            <OutputField Name="CHANGEDBY_USERNAME" Caption="Changed by user name" Category="System Fields">
                <LookupInfo>
                    <SimpleDataList SimpleDataListID="cea15e1c-e455-41be-9ecf-6b5453fa96a4" />
                </LookupInfo>
            </OutputField>
            <OutputField Name="DATEADDED" Caption="Date added" Category="System Fields" DataType="Date" />
            <OutputField Name="DATECHANGED" Caption="Date changed" Category="System Fields" DataType="Date" />
            <OutputField Name="TSLONG" Caption="Timestamp value" IsHidden="true" Category="System Fields" DataType="Long" />
        </OutputFields>
    </Output>
</QueryViewSpec>