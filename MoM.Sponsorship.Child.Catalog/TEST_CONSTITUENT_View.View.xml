		<common:FormFields>
			<common:FormField FieldID="FIELD1" Caption="Area Description" DataType="String" MaxLength="1000" />
			<common:FormField FieldID="AGE" Caption="Age" DataType="TinyInt"  />
		</common:FormFields>
	</common:FormMetaData>

  <common:DataFormExtension DataFormInstanceID="48cefdc3-a719-4fef-bd61-3108a5971d2b" RenderStyle="AfterParent" TabCaption="Age of Constituent" />
  
  
