<DataListSpec 
    xmlns:c="bb_appfx_commontypes"
    ID="d595b654-a3e3-4009-9a3a-e65e77a02840"
    Name="TESTINGONLY List"
    Description="Returns all TESTINGONLY records."
    Author="memphis-PC\Administrator"
    c:SecurityUIFolder="TESTINGONLY"
    xmlns="bb_appfx_datalist"
    >
    <SPDataList SPName="USP_DATALIST_TESTINGONLY">
        <c:CreateProcedureSQL>
<![CDATA[
create procedure dbo.USP_DATALIST_TESTINGONLY
as
	set nocount on;

	select 
        USR_TESTINGONLY.[ID],
        USR_TESTINGONLY.[TESTMEMO],
        USR_TESTINGONLY.[TESTINGTEXT]
	from 
        dbo.USR_TESTINGONLY

]]>
</c:CreateProcedureSQL>
    </SPDataList>
    <Output>
        <OutputFields>
            <OutputField FieldID="ID" Caption="ID" IsHidden="true" DataType="Guid" />
            <OutputField FieldID="TESTMEMO" Caption="Testmemo" DataType="String" />
            <OutputField FieldID="TESTINGTEXT" Caption="Testingtext" DataType="String" />
        </OutputFields>
    </Output>
</DataListSpec>