<DataListSpec
	xmlns="bb_appfx_datalist"
	xmlns:common="bb_appfx_commontypes"
	ID="3db0165b-41c6-400c-b8aa-fd2f99a4dfaa"
	Name="Sponsorship Opportunity Eligibility Package Version Data List"
	Description="Used to display the version of the package deployment."
	Author="Cary Mayeda"
	common:SecurityUIFolder="Sponsorship\Business Process\Eligibility" >

	<!-- define the SP that returns the data for the datalist -->
	<SPDataList SPName="USR_USP_DATALIST_SPONSORSHIPOPPORTUNITYELIGIBILITYPACKAGEVERSION">
		<common:CreateProcedureSQL>
			<![CDATA[
			
create procedure dbo.USR_USP_DATALIST_SPONSORSHIPOPPORTUNITYELIGIBILITYPACKAGEVERSION
as
begin

	select 'Version #VersionNumber#'

end

]]>
		</common:CreateProcedureSQL>
	</SPDataList>


	<!-- describe any parameters (other than the context ID) defined on the SP
	<Parameters>
		<common:FormMetaData>
			<common:FormFields>
				<common:FormField FieldID="PARAMETER1" Caption="Paramter1" DataType="REPLACE_WITH_DATATYPE"/>
			</common:FormFields>
		</common:FormMetaData>
	</Parameters>-->

	<!-- define the output fields in the list -->
	<Output>
		<OutputFields>
			<OutputField FieldID="Version" Caption="Sponsorship Eligibilty Package Version" DataType="String" />
		</OutputFields>
	</Output>

</DataListSpec>
